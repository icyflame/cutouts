.col-md-3
.center.col-md-6
  = render 'shared/article_single_show', :object => @thisOne
  %h1 Share this Cutout!
  =form_tag("#{article_path(@thisOne)}/share", method: :post) do
    = text_field_tag "emails", "", { :placeholder => "Please enter comma separated email IDs to share with here (maximum 5)" }
    = submit_tag "Share with these email IDs"
